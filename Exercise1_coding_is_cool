#include <iostream>
using namespace std;

int main() {
    char op;
    float num1, num2;

    cin >> op >> num1 >> num2;

    switch (op) {
        case '+': cout << num1 + num2; break;
        case '-': cout << num1 - num2; break;
        case '*': cout << num1 * num2; break;
        case '/': cout << num1 / num2; break;
        default:  cout << "Error!";
    }

    return 0;
}
