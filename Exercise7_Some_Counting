#include <iostream>
using namespace std;

int main() {
    double num1, num2, num3, num4, num5;
    char op;

    cout << "Enter 5 numbers: ";
    cin >> num1 >> num2 >> num3 >> num4 >> num5;

    cout << "Enter an operator (+, -, *, /): ";
    cin >> op;

    switch (op) {
        case '+':
            cout << "Result: " << num1 + num2 + num3 + num4 + num5 << endl;
            break;

        case '-':
            cout << "Result: " << num1 - num2 - num3 - num4 - num5 << endl;
            break;

        case '*':
            cout << "Result: " << num1 * num2 * num3 * num4 * num5 << endl;
            break;

        case '/':
            if (num2 != 0 && num3 != 0 && num4 != 0 && num5 != 0) {
                cout << "Result: " << num1 / num2 / num3 / num4 / num5 << endl;
            } else {
                cout << "ERROR\nCannot divide by zero" << endl;
            }
            break;

        default:
            cout << "Invalid operator!" << endl;
    }

    return 0;
}
